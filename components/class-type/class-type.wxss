.pouponList {
  width: 100%;
  overflow: hidden;
}

.class-header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  font-size: 32rpx;
  color: #222222;
  background: #EAECEF;
}

.class-header .cl {
  padding: 0 28rpx;
  height: 100%;
  line-height: 100rpx;
  box-sizing: border-box;
}

.class-header .class-title {
  font-size: 32rpx;
  font-weight: 500;
  color: #222222;
}

.class-header .class-cancel{
  font-size: 32rpx;
  color: #999999;
}
.class-header .class-confrim{
  font-size: 32rpx;
  color:#2564F6;
}

.class-content {
  width: calc(100% + 12rpx);
}

.pouponList .scroll-box {
  padding: 0 40rpx;
}

.pouponList .typeitem {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 110rpx;
  line-height: 110rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #EDEDED;
  font-size: 36rpx;
  color: #222222;
}

.pouponList .typeitem image {
  width: 40rpx;
  height: 40rpx;
}