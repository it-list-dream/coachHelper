page {
  background: #FEFEFE;
}

.fitness-bg {
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 350rpx;
  padding: 0 30rpx;
  background: #2664F6;
  box-sizing: border-box;
}

.fitness-member {
  margin-top: 30rpx;
  text-align: center;
}

.fitness-member image {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}

.fitness-member .name {
  margin-top: 6rpx;
  font-size: 36rpx;
  color: #FFFFFF;
}

.fitness-comparison {
  margin-top: 40rpx;
  display: flex;
}

.fitness-comparison .addbodytest,
.addappoinment {
  width: 230rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background: #FFFFFF;
  border-radius: 12rpx;
  font-size: 28rpx;
  color: #2664F6;
}

.addappoinment {
  margin-left: 20rpx;
}

.addappoinment image {
  width: 24rpx;
  height: 24rpx;
  vertical-align: middle;
}

.addappoinment text {
  vertical-align: middle;
  margin-left: 4rpx;
}

.fitness-comparison .comparisonbtn {
  margin-left: 60rpx;
  width: 156rpx;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  border: 1px solid #FFFFFF;
  border-radius: 30rpx;
  font-size: 28rpx;
  color: #FFFFFF;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.datepicker {
  display: flex;
  align-items: center;
  width: 100%;
  height: 130rpx;
  font-size: 36rpx;
  color: #2765F6;
}

.datepicker .pick-box:first-child {
  margin-left: 72rpx;
}

.datepicker .line {
  width: 70rpx;
  height: 4rpx;
  background: #A1ACC6;
  margin-left: 72rpx;
  margin-right: 100rpx;
}

.picker {
  vertical-align: middle;
}

.picker image {
  width: 36rpx;
  height: 36rpx;
  vertical-align: middle;
}

.fitness-item {
  position: relative;
  margin: 0 auto 30rpx;
  width: 690rpx;
  height: 214rpx;
  padding: 0 22rpx;
  box-sizing: border-box;
  background: #FFFFFF;
  box-shadow: 0px 0px 14rpx 0px rgba(0, 34, 112, 0.08);
  border-radius: 12rpx;
}

.icon-bg {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}

.f-box {
  position: relative;
  z-index: 10;
}

.test-report {
  display: flex;
  height: 86rpx;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1px solid #EDEDED;
}

.test-report .report-info {
  margin-left: 12rpx;
  position: relative;
  flex: 1;
}

.test-report .report-info::before {
  content: '';
  display: inline-block;
  width: 6rpx;
  height: 26rpx;
  background: #2765F6;
}

.test-report .report-info text:first-child {
  position: relative;
  padding-left: 22rpx;
  display: inline-block;
  font-size: 28rpx;
  color: #2765F6;
}

.report-info text:first-child::after {
  content: '';
  width: 6rpx;
  height: 26rpx;
  background: #2765F6;
}

.test-report .report-info text:last-child {
  margin-left: 28rpx;
  font-size: 22rpx;
  color: #666666;
}

.test-report .icon-box {
  width: 34rpx;
  height: 34rpx;
}

.test-report .icon-box image {
  width: 100%;
  height: 100%;
}

.test-report .icon-box .icon-border {
  display: block;
  width: 34rpx;
  height: 34rpx;
  border: 1px solid #BFBFBF;
  border-radius: 50%;
  box-sizing: border-box;
}

.test-report .m36 {
  width: 36rpx;
  height: 36rpx;
}

.test-report-data {
  display: flex;
  font-size: 28rpx;
  color: #333333;
}

.test-report-data text:first-child {
  font-weight: bold;
}

.test-report-data .test-report-item {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  width: 30%;
  height: 128rpx;
  padding: 28rpx 0;
  box-sizing: border-box;
}

.bottombtn {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background-color: #FFFFFF;
}

.bottombtn .complete {
  margin: 0 auto;
  width: 620rpx;
  height: 86rpx;
  text-align: center;
  line-height: 86rpx;
  background: #2564F6;
  border-radius: 12rpx;
  border: none;
  font-size: 32rpx;
  color: #FFFFFF;
}