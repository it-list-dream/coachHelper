/*  */
.savetemplate {
  width: 690rpx;
  height: 552rpx;
  background: #FFFFFF;
  border-radius: 8px;
}

.savetitle {
  padding: 50rpx 0;
  text-align: center;
  font-size: 32rpx;
  color: #010101;
}

.save-content {
  padding: 0 50rpx;
  margin-top: 60rpx;
  display: flex;
  justify-content: space-between;
  font-size: 28rpx;
  color: #999999;
}

.save-content .template-name {
  font-size: 28rpx;
  color: #000000;
}

.save-content .input {
  text-align: right;
  color: #333333;
}

.btnlist {
  display: flex;
  justify-content: space-around;
  margin-top: 180rpx;
}

.btnlist .cancel {
  border: 1px solid #CECECE;
  border-radius: 34rpx;
  color: #B3B3B3;
  background-color: #FFFFFF;
}

.btnlist .save {
  background: #2564F6;
  color: #FFFFFF;
}

.btnlist .btn {
  width: 220rpx;
  height: 65rpx;
  line-height: 65rpx;
  text-align: center;
  border-radius: 34rpx;
  font-size: 28rpx;
}
