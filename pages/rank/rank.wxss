.addpeople  {
  text-align: center;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #5F697E; 
}

.elimination{
  margin-top: 12rpx;
}

.addpeople .addpeople-item text:last-child {
  margin-top: 15rpx;
}

.rank-table .rank-th {
  display: flex;
  align-items: center;
  height: 106rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #333333;
}

.rank-table .rank-th text,
.rank-table .rank-tr .rank-rows {
  flex: 1;
  text-align: center;
}

.rank-table .rank-tr {
  display: flex;
  align-items: center;
  height: 106rpx;
  font-size: 28rpx;
  color: #333333;
  border-bottom: 1px solid #EDEDED;
  box-sizing: border-box;
}

.rank-table .rank-tr text:last-child {
  color: #2564F6;
}

.sort {
  text-align: center;
}

.sort image {
  width: 42rpx;
  height: 47rpx;
}